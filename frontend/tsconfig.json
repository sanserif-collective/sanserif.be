{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "layouts/*": ["src/layouts/*"],
      "components/*": ["src/components/*"],
      "sections/*": ["src/sections/*"],
      "types/*": ["src/types/*"],
      "queries/*": ["src/queries/*"],
      "scripts/*": ["src/scripts/*"],
      "utilities/*": ["src/utilities/*"],
      "env": ["src/env"],
      "strapi": ["src/strapi"]
    }
  },
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"]
}
